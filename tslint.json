{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [true, "spaces", 2],
    "quotemark": [true, "single"],
    "ordered-imports": false,
    "max-classes-per-file": false,
    "no-console": false,
    "variable-name": false,
    "object-literal-sort-keys": false,
    "max-line-length": false,
    "no-namespace": false,
    "arrow-parens": false,
    "no-angle-bracket-type-assertion": false,
    "whitespace": [
      true, 
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-type-operator",
      "check-preblock",
      "check-postbrace"
    ]
  },
  "rulesDirectory": []
}
